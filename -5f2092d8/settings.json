{
    "editor.semanticTokenColorCustomizations": {
        "[Tokyo Night Dark]": {
            "enabled": false,
            "rules": {
                "*.builtin": "#0db9d7",
                "selfParameter": "#F7768E",
                "*.declaration.readonly": "#3dfff5",
                "*.readonly": "#3dfff5"
            }
        }, 
        "[Best Themes - Cosmic]": {
            "enabled": false
        }
